{
  "extends": [
    "@tsconfig/node22/tsconfig.json",
    "@tsconfig/strictest/tsconfig.json"
  ],
  "compilerOptions": {
    "strictNullChecks": true,
    "verbatimModuleSyntax": false,
    "skipLibCheck": true,
    "composite": true
  },
  "include": ["**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules", "../../node_modules"]
}
